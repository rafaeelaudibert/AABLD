<!DOCTYPE html>
<html>
  <head>
    <title>Aabld</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% "<p class='alert alert-success'>#{notice}></p>" if notice %>
    <% "<p class='alert alert-danger'>#{alert}></p>" if alert %>
    
    <%= yield %>
  </body>
</html>
